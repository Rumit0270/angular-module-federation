{
  "name": "angular-module-federation",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:container",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:container": "ng serve container --port 5000",
    "start:first": "ng serve first --port 5001",
    "start:second": "ng serve second --port 5002",
    "start:all": "npm run build:shared-lib &&  concurrently \"npm run start:container\" \"npm run start:first\" \"npm run start:second\" ",
    "build:shared-lib": "ng build --project=shared-lib"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^16.0.0",
    "@angular-architects/module-federation-runtime": "^16.0.0",
    "@angular-architects/module-federation-tools": "^16.0.0",
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.4",
    "@angular/cli": "^16.2.4",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "concurrently": "^8.2.2",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.2.0",
    "typescript": "~5.1.3",
    "ngx-build-plus": "^16.0.0"
  }
}
